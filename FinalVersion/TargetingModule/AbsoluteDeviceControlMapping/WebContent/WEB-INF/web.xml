<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Spidio DMP Device Control Webapp</display-name>
  <servlet>
    <servlet-name>DefaultCampaign</servlet-name>
    <servlet-class>com.spidio.DataLogix.web.DefaultCampaign</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DefaultCampaign</servlet-name>
    <url-pattern>/DefaultCampaign</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EraseCampaignData</servlet-name>
    <servlet-class>com.spidio.DataLogix.web.EraseCampaignData</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EraseCampaignData</servlet-name>
    <url-pattern>/EraseCampaignData</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MapFingerprint</servlet-name>
    <servlet-class>com.spidio.DataLogix.web.MapFingerprint</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MapFingerprint</servlet-name>
    <url-pattern>/MapFingerprint</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GenerateUrl</servlet-name>
    <servlet-class>com.spidio.DataLogix.web.GenerateUrl</servlet-class>
  </servlet>
   <servlet-mapping>
    <servlet-name>GenerateUrl</servlet-name>
    <url-pattern>/GenerateUrl</url-pattern>
  </servlet-mapping>
  
</web-app>